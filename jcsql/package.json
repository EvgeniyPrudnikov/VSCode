{
    "name": "jcsql",
    "displayName": "JCSQL",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.addConnection",
        "onCommand:extension.deleteConnection",
        "onCommand:extension.showConnections",
        "onCommand:extension.runCode"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.addConnection",
                "title": "Add Connection",
                "category": "JCSQL"
            },
            {
                "command": "extension.deleteConnection",
                "title": "Delete Connection",
                "category": "JCSQL"
            },
            {
                "command": "extention.showConnections",
                "title": "Show Connections",
                "category": "JCSQL"
            },
            {
                "command": "extension.runCode",
                "title": "Run Code",
                "category": "JCSQL"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.38.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "tslint": "^5.12.1",
        "vscode-test": "^1.2.0"
    },
    "dependencies": {
        "python-shell": "^1.0.8"
    }
}
